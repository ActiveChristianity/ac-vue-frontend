{
  "version": 2,
  "regions": ["cdg1"],
  "routes": [
    {
      "src": "/service-worker.js",
      "headers": { "Cache-Control": "max-age=0, must-revalidate" },
      "continue": true
    },
    {
      "src": "/assets/js/service-worker.js",
      "headers": { "Cache-Control": "max-age=0, must-revalidate" },
      "continue": true
    },
    {
      "src": "/manifest.json",
      "headers": { "Cache-Control": "max-age=0, must-revalidate" },
      "continue": true
    },
    {
      "src": "/topic",
      "dest": "/topics"
    },
    {
      "src": "/(.*)",
      "headers": { "Cache-Control": "max-age=604800, must-revalidate" },
      "dest": "/$1"
    },
    {
      "handle": "filesystem",
      "headers": { "Cache-Control": "max-age=604800, must-revalidate" }
    },
    {
      "src": "/.*",
      "status": 200,
      "headers": { "Cache-Control": "max-age=604800, must-revalidate" },
      "dest": "index.html"
    }
  ]
}